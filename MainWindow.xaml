<Window x:Class="HashmapApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HashmapApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtKey" HorizontalAlignment="Center" Margin="0,146,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="200" Height="30"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" Width="200" Height="30"/>
        <Button x:Name="addbt" Content="add" HorizontalAlignment="Left" Margin="194,268,0,0" VerticalAlignment="Top" Width="100" Height="30" Click="addbt_Click"/>
        <Button x:Name="removebt" Content="remove" HorizontalAlignment="Center" Margin="0,268,0,0" VerticalAlignment="Top" Width="100" Height="30" Click="removebt_Click"/>
        <Button x:Name="showbt" Content="show" HorizontalAlignment="Left" Margin="519,268,0,0" VerticalAlignment="Top" Width="100" Height="30" Click="showbt_Click"/>

    </Grid>
</Window>
